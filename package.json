  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "react",
    "preact",
    "library",
    "module",
    "create-react-app",
    "cli",
    "component",
    "rollup",
    "babel",
    "publish"

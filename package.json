{
  "name": "krul-cli",
  "version": "0.0.10",
  "description": "CLI for create project",
  "repository": "",
  "author": "Go7hic",
  "main": "index.js",
  "license": "MIT",
  "reveal": true,
  "bin": {
    "krul-cli": "index.js"
  },
  "scripts": {
    "test": "ava -v && standard *.js lib/*.js",
    "lint-staged": "lint-staged",
    "release": "standard-version"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "husky": {

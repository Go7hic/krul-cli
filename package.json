  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "conf": "^2.2.0",
    "cp-file": "^6.0.0",
    "execa": "^1.0.0",
    "git-config-path": "^2.0.0",
    "github-username": "^4.1.0",
    "globby": "^9.0.0",
    "handlebars": "^4.1.0",
    "inquirer": "^6.2.2",
    "make-dir": "^2.0.0",
    "ora": "^3.1.0",
    "p-each-series": "^1.0.0",
    "parse-git-config": "^3.0.0",
    "validate-npm-package-name": "^3.0.0",
    "which": "^1.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "ava": "^1.2.1",
    "eslint": "^6.1.0",
    "husky": "^3.0.3",
    "lerna": "^3.16.4",
    "lint-staged": "^9.2.1",
    "rmfr": "^2.0.0",
    "standard-version": "^7.0.0"
  }
